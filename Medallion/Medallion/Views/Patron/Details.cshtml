@model Patron
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Medallion Theater</title>
    <link rel="stylesheet" href="./Register.css">
</head>
<body>
    <div class="wrapper" >
        <form action="" method="post">
            <h1>Register Patron</h1>
            <div class="form-group">
                <label for="fname">Patron Number</label>
                <input type="text" name="PatronId" class="fname" value="@Model.PatronId">
            </div>
            <div class="form-group">
                <label for="fname">First Name</label>
                <input type="text" name="FirstName" class="fname" value="@Model.FirstName">
            </div>
            <div class="form-group">
                <label for="lname">Last Name</label>
                <input type="text" name="LastName" class="fname" value="@Model.LastName">
            </div>
            <div class="form-group">
                <label for="address">Street Address</label>
                <input type="text" name="Address" class="fname" value="@Model.Address">
            </div>
            <div class="form-group">
                <label for="city">City</label>
                <input type="text" name="City" class="fname" value="@Model.City">
            </div>
            <div class="form-group">
                <label for="state">State</label>
                <input type="text" name="Region" class="fname" value="@Model.Region">
            </div>
            <div class="form-group">
                <label for="zip">Zip</label>
                <input type="text" name="ZipCode" class="fname" value="@Model.ZipCode">
            </div>
            <div class="form-group">
                <label for="phone">Phone Number</label>
                <input type="text" name="CellPhone" class="fname" value="@Model.CellPhone">
            </div>
            <div class="form-group">
                <label for="email">Email</label>
                <input type="text" name="Email" class="fname" value="@Model.Email">
            </div>
            <div class="actions">
                <input   value="Delete" style="width:10rem;" class="btn btn-danger">
                <input type="Put" value="Update" style="width:10rem;background-color:orange;" class="btn btn_submit ">
            </div>
        </form>
    </div>
</body>
</html>